{
    "name": "midnite81/alphacon",
    "description": "Alphacon (or AlphabetConversion) converts characters in strings into different formats such as Nato Strings or Morse Code.",
    "type": "library",
    "require": {
        "php": ">7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "orchestra/testbench": "^5.3",
        "laravel/framework": "^5.0|^6.0|^7.0",
        "satooshi/php-coveralls": "^2.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Rogers",
            "email": "webdev@centralblue.co.uk"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Midnite81\\Alphacon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Midnite81\\Alphacon\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Midnite81\\Alphacon\\AlphaconServiceProvider"
            ]
        }
    }
}
